<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro">

  <xacro:include filename="$(find omnirob_robin_description)/urdf/lwa/lwa.transmission.xacro" />
  <xacro:include filename="$(find omnirob_robin_description)/urdf/lwa/lwa.gazebo.xacro" />

  <xacro:macro name="lwa" params="parent name *origin">

    <!-- joint between base_link and arm_0_link -->
    <joint name="${name}/joint_0" type="fixed" >
      <insert_block name="origin" />
      <parent link="${parent}" />
      <child link="${name}/link_0" />
    </joint>

    <link name="${name}/link_0">
      <inertial>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <mass value="1.0"/>
        <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1" />
      </inertial>

      <visual>
        <origin xyz="0 0 0.004" rpy="0 0 0" />
        <geometry>
          <cylinder radius="0.045" length="0.08"/>
        </geometry>
        <material name="Schunk/DarkGolden" />
      </visual>

      <collision>
        <origin xyz="0 0 0.005" rpy="0 0 0" />
        <geometry>
          <box size="0.1 0.1 0.1"/>
        </geometry>
      </collision>
    </link>

    <!-- joint between arm_0_link and arm_1_link -->
    <joint name="${name}/joint_1" type="revolute">
      <origin xyz="0 0 -0.250" rpy="0 0 0"/>
      <parent link="${name}/link_0"/>
      <child link="${name}/link_1"/>
      <axis xyz="0 0 1"/>     
      <dynamics damping="0.5" friction="1.0" />
      <limit effort="100" velocity="2.0" lower="-3.0" upper="3.0"/>
      <safety_controller k_position="20" k_velocity="50" soft_lower_limit="${-3.0 + 0.01}" soft_upper_limit="${3.0 - 0.01}" />
    </joint>

    <link name="${name}/link_1">
      <inertial>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <mass value="5.0"/>
        <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1" />
      </inertial>
      
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://omnirob_robin_description/meshes/lwa/lwa1.stl" />
        </geometry>
        <material name="Schunk/DarkGolden" />
      </visual>
      
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://omnirob_robin_description/meshes/lwa/lwa1.stl" />
        </geometry>
      </collision>
    </link>

    <!-- joint between arm_1_link and arm_2_link -->  
    <joint name="${name}/joint_2" type="revolute">
      <origin xyz="0 0 0" rpy="1.5708 0 0"/>
      <parent link="${name}/link_1"/>
      <child link="${name}/link_2"/>
      <axis xyz="0 0 1"/>
      <dynamics damping="0.5" friction="1.0" />
      <limit effort="300" velocity="2.0" lower="-1.9" upper="1.9"/>
      <safety_controller k_position="20" k_velocity="50" soft_lower_limit="${-1.9 + 0.01}"   soft_upper_limit="${1.9 - 0.01}" />
    </joint>

    <link name="${name}/link_2">
      <inertial>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <mass value="5.0"/>
        <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1" />
      </inertial>
      
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://omnirob_robin_description/meshes/lwa/lwa2.stl" />
        </geometry>
        <material name="Schunk/DarkGolden" />
      </visual>
      
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://omnirob_robin_description/meshes/lwa/lwa2.stl"/>
        </geometry>
      </collision>
    </link>

    <!-- joint between arm_2_link and arm_3_link -->  
    <joint name="${name}/joint_3" type="revolute">
      <origin xyz="0 -0.408 0" rpy="-1.5708 0 0"/>
      <parent link="${name}/link_2"/>
      <child link="${name}/link_3"/>
      <axis xyz="0 0 1"/>
      <dynamics damping="0.5" friction="1.0" />
      <calibration rising="${arm_3_ref}" falling="0.0"/>
      <limit effort="300" velocity="2.0" lower="-3.14" upper="3.14"/>
      <safety_controller k_position="20" k_velocity="25" soft_lower_limit="${-3.14 + 0.1}" soft_upper_limit="${3.14 - 0.1}" />
    </joint>

    <link name="${name}/link_3">
      <inertial>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <mass value="4.0"/>
        <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1" />
      </inertial>
      
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://omnirob_robin_description/meshes/lwa/lwa3.stl" />
        </geometry>
        <material name="Schunk/DarkGolden" />
      </visual>
      
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://omnirob_robin_description/meshes/lwa/lwa3.stl"/>
        </geometry>
      </collision>
    </link>  

    <!-- joint between arm_3_link and arm_4_link -->
    <joint name="${name}/joint_4" type="revolute">
      <origin xyz="0 0 0" rpy="1.5708 0 0" />
      <parent link="${name}/link_3"/>
      <child link="${name}/link_4"/>
      <axis xyz="0 0 1" />
      <dynamics damping="0.5" friction="1.0" />
      <calibration rising="${arm_4_ref}" falling="0.0"/>
      <limit effort="300" velocity="2.0" lower="-2.44" upper="2.44"/>
      <safety_controller k_position="20" k_velocity="25" soft_lower_limit="${-2.44 + 0.01}" soft_upper_limit="${2.44 - 0.01}" />
    </joint>

    <link name="${name}/link_4">
      <inertial>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <mass value="3.5"/>
        <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1" />
      </inertial>
      
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://omnirob_robin_description/meshes/lwa/lwa4.stl" />
        </geometry>
        <material name="Schunk/DarkGolden" />
      </visual>
      
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://omnirob_robin_description/meshes/lwa/lwa4.stl"/>
        </geometry>
      </collision>
    </link>

    <!-- joint between arm_4_link and arm_5_link -->
    <joint name="${name}/joint_5" type="revolute">
      <origin xyz="0 -0.3165 0" rpy="-1.5708 0 0" />
      <parent link="${name}/link_4"/>
      <child link="${name}/link_5"/>
      <axis xyz="0 0 1" />
     <dynamics damping="0.5" friction="1.0" />
      <calibration rising="${arm_5_ref}" falling="0.0"/>
      <limit effort="300"  velocity="2.0" lower="-3" upper="3" />
      <safety_controller k_position="20" k_velocity="25" soft_lower_limit="${-3 + 0.01}" soft_upper_limit="${3 - 0.01}" />
    </joint>

    <link name="${name}/link_5">
      <inertial>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <mass value="2.0"/>
        <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1" />
      </inertial>
      
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://omnirob_robin_description/meshes/lwa/lwa5.stl" />
        </geometry>
        <material name="Schunk/DarkGolden" />
      </visual>
      
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://omnirob_robin_description/meshes/lwa/lwa5.stl"/>
        </geometry>
      </collision>
    </link>

    <!-- joint between arm_5_link and arm_6_link -->
    <joint name="${name}/joint_6" type="revolute">
      <origin xyz="0 0 0" rpy="1.5708 0 0" />
      <parent link="${name}/link_5"/>
      <child link="${name}/link_6"/>
      <axis xyz="0 0 1" />
      <dynamics damping="0.5" friction="1.0" />
      <calibration rising="${arm_6_ref}" falling="0.0"/>
      <limit effort="300" velocity="2.0" lower="-2.44" upper="2.44" />
      <safety_controller k_position="20" k_velocity="25" soft_lower_limit="${-2.44 + 0.01}" soft_upper_limit="${2.44 - 0.01}" />
    </joint>

    <link name="${name}/link_6">
      <inertial>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <mass value="2.0"/>
        <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1" />
      </inertial>
      
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://omnirob_robin_description/meshes/lwa/lwa6.stl" />
        </geometry>
        <material name="Schunk/DarkGolden" />
      </visual>
      
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://omnirob_robin_description/meshes/lwa/lwa6.stl"/>
        </geometry>
      </collision>
    </link>

    <!-- joint between arm_6_link and arm_7_link -->
    <joint name="${name}/joint_7" type="revolute">
      <origin xyz="0 -0.1927 0" rpy="-1.5708 0 0" />
      <parent link="${name}/link_6"/>
      <child link="${name}/link_7"/>
      <axis xyz="0 0 1" />
      <dynamics damping="0.5" friction="1.0" />
      <calibration rising="${arm_7_ref}" falling="0.0"/>
      <limit effort="100" velocity="2.0" lower="-3" upper="3" />
      <safety_controller k_position="20" k_velocity="25" soft_lower_limit="${-3 + 0.01}" soft_upper_limit="${3 - 0.01}" />
    </joint>

    <link name="${name}/link_7">
      <inertial>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <mass value="1.0"/>
        <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1" />
      </inertial>
      
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://omnirob_robin_description/meshes/lwa/lwa7.stl" />
        </geometry>
        <material name="Schunk/DarkGolden" />
      </visual>
      
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://omnirob_robin_description/meshes/lwa/lwa7.stl"/>
        </geometry>
      </collision>
    </link>    
    
    
    <xacro:if value="${simulated}">
      <xacro:lwa_transmission name="${name}" /> 
      <xacro:lwa_gazebo name="${name}" />
    </xacro:if>
    
  </xacro:macro>

</robot>
